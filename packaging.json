{
    "clean": "./dist",
    "yuic": [
        "-jar",
        "/usr/local/bin/yuicompressor-2.4.7/build/yuicompressor-2.4.7.jar",
        "--line-break",
        "1000"
    ],
    "packs": [
        {
            "dest": "dist/dAmnVideo.js",
            "mini": "dist/dAmnVideo.min.js",
            "files": "src/"
        },
        {
            "sass": true,
            "options": [
                "--style",
                "expanded"
            ],
            "dest": "dist/dAmnVideo.css",
            "mini": "dist/dAmnVideo.min.css",
            "files": "css/dAmnVideo.scss"
        }
    ],
    "post": ["./mvlh"]
}